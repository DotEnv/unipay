{
    "name": "dotenv/unipay",
    "description": "An simple payment gateway api",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.0|^7.1",
        "illuminate/http": "~5.5.0|~5.6",
        "illuminate/support": "~5.5.0|~5.6",
        "illuminate/database": "~5.5.0|~5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*|^6.3",
        "orchestra/testbench": "~3.4.0|~3.5.0|~3.6",
        "orchestra/database": "~3.4.0|~3.5.0|~3.6"
    },
    "autoload": {
        "psr-4": {
            "DotEnv\\UniPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UniPay\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tiago Perrelli",
            "email": "tiagoyg@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "DotEnv\\UniPay\\Providers\\UniPayServiceProvider"
            ]
        }
    }    
}
